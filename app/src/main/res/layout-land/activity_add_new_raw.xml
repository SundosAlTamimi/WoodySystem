                            android:padding="7dp"
                            app:srcCompat="@drawable/pic" />

                        <ImageView
                            android:id="@+id/addNewRaw_image15"
                            android:layout_width="0dp"
                            android:layout_height="50dp"
                            android:layout_marginHorizontal="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_background"
                            android:padding="7dp"
                            app:srcCompat="@drawable/pic" />

                    </LinearLayout>
                </ScrollView>

                <TextView
                    android:id="@+id/addNewRaw_add_photo"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1.4"
                    android:background="@drawable/frame_shape"
                    android:drawableStart="@drawable/ic_add_a_photo"
                    android:drawableLeft="@drawable/ic_add_a_photo"
                    android:fontFamily="serif"
                    android:layout_marginHorizontal="5dp"
                    android:paddingStart="45dp"
                    android:paddingLeft="45dp"
                    android:layout_gravity="end"
                    android:textColor="@color/white"/>
                <TextView
                    android:id="@+id/addNewRaw_add_button"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1.4"
                    android:background="@drawable/frame_shape"
                    android:drawableStart="@drawable/ic_add_white_32dp"
                    android:drawableLeft="@drawable/ic_add_white_32dp"
                    android:fontFamily="serif"
                    android:paddingStart="40dp"
                    android:paddingLeft="40dp"
                    android:layout_marginHorizontal="5dp"
                    android:layout_gravity="end"
                    android:textColor="@color/white"/>


            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/addNewRaw_table_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="1dp"
            android:layout_marginLeft="1dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="1dp"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="1"
            android:background="@color/alpha_black"
            android:orientation="vertical">

            <TableLayout
                android:id="@+id/addNewRaw_table_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="3dp"
                android:layout_marginRight="3dp" />

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableLayout
                    android:id="@+id/addNewRaw_table"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="3dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="3dp"
                    android:layout_marginRight="3dp" />

            </ScrollView>
        </LinearLayout>

        <View
            android:id="@+id/inventory_report_linear6"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="5dp"
            android:background="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/inventory_report_linear7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="5dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:background="@drawable/alpha_frame"
                android:orientation="horizontal"
                app:layout_constraintTop_toBottomOf="@+id/addToInventory_linear_three">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:fontFamily="serif"
                    android:gravity="start|center_vertical"
                    android:paddingHorizontal="10dp"
                    android:text="@string/reject"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:background="@drawable/frame_shape"/>

                <TextView
                    android:id="@+id/addNewRaw_total_rejected"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:ems="10"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:singleLine="true"
                    android:textColor="@color/orange"
                    android:textSize="15sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/alpha_frame"
                android:orientation="horizontal"
                app:layout_constraintTop_toBottomOf="@+id/addToInventory_linear_three">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:fontFamily="serif"
                    android:gravity="start|center_vertical"
                    android:paddingHorizontal="5dp"
                    android:text="@string/net"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:background="@drawable/frame_shape"/>

                <TextView
                    android:id="@+id/addNewRaw_total_bundles"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:ems="10"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:nextFocusDown="@id/addNewRaw_acceptance_location"
                    android:singleLine="true"
                    android:textColor="@color/orange"
                    android:textSize="15sp" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">




                <Button
                    android:id="@+id/addNewRaw_acceptRow_pdf"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="2"
                    android:background="@drawable/frame_shape"
                    android:fontFamily="serif"
                    android:text="@string/pdf"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    android:visibility="gone"
                    android:layout_marginLeft="5dp" />
                <Button
                    android:id="@+id/addNewRaw_acceptRow_excel"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="2"
                    android:background="@drawable/frame_shape"
                    android:fontFamily="serif"
                    android:text="@string/excel"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    android:visibility="gone"
                    android:layout_marginLeft="5dp" />

                <Button
                    android:id="@+id/addNewRaw_acceptRow_done"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="2"
                    android:background="@drawable/frame_shape"
                    android:fontFamily="serif"
                    android:text="@string/save"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    android:layout_marginLeft="5dp" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>