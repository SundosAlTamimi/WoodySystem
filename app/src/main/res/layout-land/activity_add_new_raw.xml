                            android:layout_weight="1"
                            android:background="@drawable/border_background"
                            android:padding="7dp"
                            app:srcCompat="@drawable/pic" />

                        <ImageView
                            android:id="@+id/addNewRaw_image11"
                            android:layout_width="0dp"
